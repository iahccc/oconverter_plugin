<idea-plugin>
    <id>top.iahc.oconverter_plugin</id>
    <name>OConverter</name>
    <vendor email="" url="">IAHC</vendor>

    <idea-version since-build="201.0"/>

    <description><![CDATA[
    VO、DTO、VO、PO、DO...对象转换插件.<br>
    ]]></description>


    <!-- please see https://plugins.jetbrains.com/docs/intellij/plugin-compatibility.html
         on how to target different products -->
    <depends>com.intellij.modules.platform</depends>
    <depends>com.intellij.modules.java</depends>

    <extensions defaultExtensionNs="com.intellij">
        <!-- Add your extensions here -->
    </extensions>

    <actions>
        <!-- Add your actions here -->
        <action id="oto_action" class="top.iahc.oto.action.OtoAction" text="oto" description="oto">
            <add-to-group group-id="GenerateGroup" anchor="first"/>
        </action>
    </actions>
</idea-plugin>